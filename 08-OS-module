const os = require('os');
const { memoryUsage } = require('process');

const user = os.userInfo()
console.log(user);

// System uptime


console.log(`the system uptime is: ${os.uptime()} seconds`);

const currentOS = {
    name: os.type(),
    release: os.release(),
    totalMem: os.totalmem(),
    freeMem: os.freemem(),
}
console.log(currentOS)